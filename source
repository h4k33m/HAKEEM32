	game.StarterGui:SetCore("SendNotification", {
		Title = "Xanity",
		Text = " [+] Your Fly Speed Is Now, " ..(tostring(FLYSPEED))..".",
		Duration = 1,
	})
end)

L_21_:CreateButton("Fly Speed [-]", function()
