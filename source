L_19_:CreateButton("Alt Armor", function()
	local UIS = game:GetService("UserInputService")
    local Plr = game.Players.LocalPlayer    
        UIS.InputBegan:connect(function(Input)
            local MouseInput = Input.UserInputType 
            local KeyCode = Input.KeyCode 

            if KeyCode == Enum.KeyCode.LeftAlt then
              local OldPos = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position
               Plr.Character.HumanoidRootPart.CFrame = game.Workspace.Ignored.Shop["[High-Medium Armor] - $2300"].Head.CFrame
                wait(0.5)
                fireclickdetector(game.Workspace.Ignored.Shop["[High-Medium Armor] - $2300"].ClickDetector)
              game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(OldPos)
            end
        end)
end)

